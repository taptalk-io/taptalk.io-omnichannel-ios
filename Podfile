source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'
inhibit_all_warnings!
use_frameworks!

def tapTalkLive_pods
  pod 'TapTalk', :git => 'https://github.com/alfian0/taptalk.io-ios.git'
  pod 'AFNetworking'
  pod 'JSONModel'
  pod 'SDWebImage'
  pod 'PodAsset'
end

target "TapTalkLive" do
    tapTalkLive_pods
end
