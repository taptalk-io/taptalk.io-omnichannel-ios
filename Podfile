source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'
inhibit_all_warnings!
use_frameworks!

def tapTalkLive_pods
  pod 'TapTalk'
  pod 'AFNetworking', '~> 3.1.0'
  pod 'JSONModel', '~> 1.1'
  pod 'SDWebImage', '4.4.2'
  pod 'PodAsset'
end

target "TapTalkLive" do
    tapTalkLive_pods
end
